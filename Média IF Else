// Online C++ compiler to run C++ program online
#include <iostream>

using namespace std;
int main() {
float a,b,c,d;
cout<<"Digite o valor das três notas: "<<endl;
cin>>a;
cin>>b;
cin>>c;

d=(a+b+c)/3;
cout<<"Sua média é: "<<d<<" !"<<endl;
if (d>=5)
{
    cout<<"Aprovado!"<<endl;
}
else
{
    cout<<"Reprovado"<<endl;
}
    return 0;
}
